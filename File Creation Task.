ubuntu@ip-172-31-26-76:~$ mkdir home
ubuntu@ip-172-31-26-76:~$ cd home
ubuntu@ip-172-31-26-76:~/home$ mkdir my_folder
ubuntu@ip-172-31-26-76:~/home$ cd my_folder
ubuntu@ip-172-31-26-76:~/home/my_folder$ touch my_file.txt
ubuntu@ip-172-31-26-76:~/home/my_folder$ touch another_file.txt
ubuntu@ip-172-31-26-76:~/home/my_folder$ vi my_file.txt
ubuntu@ip-172-31-26-76:~/home/my_folder$ cat my_file.txt
